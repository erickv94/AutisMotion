<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/fondo"
    tools:context=".Numeros.ContandoNumerosActivity">
<TableLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <TableRow>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/los_numeros_label"
            android:layout_marginTop="30dp"/>
    </TableRow>
    <TableRow>

        <ImageView
            android:id="@+id/numerocontar"
            android:layout_width="145dp"
            android:layout_height="289dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/animal_number_2_t" />
    </TableRow>
    <LinearLayout>

        <ImageButton
            android:layout_width="110dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="60dp"
            android:background="@drawable/boton_izquierda_t" />

        <ImageButton
            android:layout_width="110dp"
            android:layout_height="60dp"
            android:layout_marginLeft="60dp"
            android:background="@drawable/boton_derecha_t" />
    </LinearLayout>
    <TableRow>
        <LinearLayout>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""/>
        </LinearLayout>
    </TableRow>
    </TableLayout>
</RelativeLayout>
